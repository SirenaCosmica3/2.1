#include <stdio.h>
#include <stdlib.h>

int main()
{
    float hora, valor, sueldo;
    printf("Ingrese la cantidad de horas trabajadas: ");
    scanf("%f",&hora);
    printf("\nIngrese el valor por hora: ");
    scanf("%f",&valor);
    sueldo=hora*valor;
    printf("\nEl valor del sueldo es de: %2.f",sueldo);
    return 0;
}
